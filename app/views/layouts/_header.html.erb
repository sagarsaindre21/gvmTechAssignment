<header class="site-header"> 
  <nav class="navbar navbar-default">
    <div class="header-inner">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="brand">
              <a href="http://www.inforest.com/" title="Example Home" rel="home"><img src="/cart.jpg" class="" border="0" height="90px;"></a>
            </div>
            <nav  id="nav-wrap" class="main-nav"> <a id="toggle-btn" href="#"><i class="fa fa-bars"></i> </a>
              <ul class="sf-menu">
                <li class="current"> <a href="/">Home</a> </li>
                <% if current_user %>
                  <li> <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %> </li>
                  <li> <%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %></li>
                   <% if current_user&.role&.name == 'vendor'%>
                     <li> <%= link_to 'Products', vendor_products_path, :class => 'navbar-link' %></li>
                   <%elsif current_user&.role&.name == 'admin'%>
                     <li> <%= link_to 'Users', admin_users_path, :class => 'navbar-link' %></li>
                     <li> <%= link_to 'Orders', admin_orders_path, :class => 'navbar-link' %></li>
                   <%end%>
                <% else %>
                  <li> <%= link_to 'Sign Up', new_user_registration_path %></li>
                  <li> <%= link_to 'Sign In', new_user_session_path %> </li>
                <% end %>
              </ul>
            </nav> 
          </div>
        </div>
      </div>
    </div>
  </nav>
</header>
